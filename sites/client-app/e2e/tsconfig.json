{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "outDir": "./dist",
    "rootDir": "./",
    "types": ["node", "@playwright/test"],
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
