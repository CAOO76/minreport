{
  "compilerOptions": {
    "target": "es2017",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": ["es2017"],
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "src/**/*.test.ts"]
}