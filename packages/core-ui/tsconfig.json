{
  "compilerOptions": {
    "target": "ES2019",
  "module": "NodeNext",
    "declaration": true,
    "declarationMap": true,
    "outDir": "lib",
    "rootDir": "src",
    "esModuleInterop": true,
  "moduleResolution": "nodenext",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"]
}